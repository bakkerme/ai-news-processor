    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./internal",
            "args": ["-persona", "LocalLLaMa"],
            "envFile": [
                "${workspaceFolder}/.env",
                "${workspaceFolder}/.envdebug"
            ]
        },
        {
            "name": "Launch Benchmark",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./bench",
            "envFile": [
                "${workspaceFolder}/.env",
                "${workspaceFolder}/.envdebug"
            ],
            "cwd": "${workspaceFolder}/bench",
        }
    ]
}
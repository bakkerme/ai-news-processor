    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch LocalLLaMa",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./",
            "args": ["-persona", "LocalLLaMa"],
            "envFile": [
                "${workspaceFolder}/.env",
            ]
        },
        {
            "name": "Launch RetroGaming",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./",
            "args": ["-persona", "RetroGaming"],
            "envFile": [
                "${workspaceFolder}/.env",
            ]
        },
        {
            "name": "Launch Benchmark",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./benchmark",
            "envFile": [
                "${workspaceFolder}/.env",
            ],
            "cwd": "${workspaceFolder}",
        }
    ]
}